{
    "name": "omnibug",
    "version": "1.18.0",
    "description": "Omnibug Extension",
    "main": "Gruntfile.js",
    "scripts": {
        "production": "grunt production",
        "beta": "grunt beta",
        "build": "grunt build",
        "test": "cross-env NODE_ENV=test grunt build-test-files && nyc --all ava",
        "postinstall": "grunt build",
        "coverage": "cross-env NODE_ENV=test nyc report -all",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix"
    },
    "ava": {
        "files": [
            "test/providers/*.js",
            "test/core/*.js"
        ],
        "require": [
            "esm",
            "./test/helpers/setup-browser-env.js"
        ],
        "babel": false
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ],
        "env": {
            "test": {
                "sourceMaps": "inline",
                "plugins": [
                    "istanbul",
                    [
                        "@babel/plugin-proposal-decorators",
                        {
                            "legacy": true
                        }
                    ],
                    "@babel/plugin-proposal-function-sent",
                    "@babel/plugin-proposal-export-namespace-from",
                    "@babel/plugin-proposal-numeric-separator",
                    "@babel/plugin-proposal-throw-expressions",
                    "@babel/plugin-syntax-dynamic-import",
                    "@babel/plugin-syntax-import-meta",
                    "@babel/plugin-proposal-class-properties",
                    "@babel/plugin-proposal-json-strings"
                ]
            }
        }
    },
    "nyc": {
        "require": [
            "@babel/register"
        ],
        "reporter": [
            "lcov",
            "text"
        ],
        "exclude": [
            "tests/**",
            "platform/**",
            ".github/**",
            "node_modules/**",
            "**/providers.js"
        ],
        "sourceMap": false,
        "instrument": false,
        "all": true
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/MisterPhilip/omnibug.git"
    },
    "keywords": [
        "omnibug"
    ],
    "author": "Philip Lawrence <philip@omnibug.io>",
    "license": "MIT",
    "readmeFilename": "README.md",
    "bugs": {
        "url": "https://github.com/MisterPhilip/omnibug/issues"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-decorators": "^7.6.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
        "@babel/plugin-proposal-function-sent": "^7.5.0",
        "@babel/plugin-proposal-json-strings": "^7.2.0",
        "@babel/plugin-proposal-numeric-separator": "^7.2.0",
        "@babel/plugin-proposal-throw-expressions": "^7.2.0",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-syntax-import-meta": "^7.2.0",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-stage-2": "^7.0.0",
        "@babel/register": "^7.9.0",
        "@fortawesome/fontawesome-free": "^5.13.0",
        "@ungap/url-search-params": "^0.1.0",
        "ava": "^1.4.1",
        "babel-plugin-istanbul": "^6.0.0",
        "browser-env": "^3.2.6",
        "coveralls": "^3.1.0",
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "esm": "^3.2.25",
        "grunt": "^1.1.0",
        "grunt-contrib-clean": "^2.0.0",
        "grunt-contrib-compress": "^1.6.0",
        "grunt-contrib-concat": "^1.0.1",
        "grunt-contrib-copy": "^1.0.0",
        "grunt-contrib-watch": "^1.1.0",
        "grunt-eslint": "^22.0.0",
        "grunt-sass": "^3.1.0",
        "grunt-text-replace": "^0.4.0",
        "node-sass": "^4.14.1",
        "nyc": "^15.0.1",
        "sinon": "^9.0.2",
        "sinon-chrome": "^3.0.0"
    },
    "homepage": "https://omnibug.io/",
    "dependencies": {
        "spectre.css": "^0.5.8",
        "yarn": "^1.22.4"
    },
    "engines": {
        "node": ">=12.16.0",
        "yarn": ">=1.15.0"
    }
}
